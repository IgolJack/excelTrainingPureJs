{
    "parser": "babel-eslint",
    "env": {
        "es2020": true,
        "browser": true,
        "node": true
    },
    "extends": ["eslint:recommended", "google"],
    "rules": {
        "semi": "off",
        "require-jsdoc": "off",
        "arrow-parens": "off",
        "comma-dangle": "off",
        "linebreak-style": "off",
        "operator-linebreak": "off",
        "max-len": "off"
    }
}